TITLE='FastTrack Schedule (for con)'
DESCRIPTION='What is happening in FastTrack - The At-Con Version'
FASTTRACKWANTS=1
QUERY="
SELECT
            DATE_FORMAT(ADDTIME('2011-01-14 00:00:00',SCH.starttime),'%a %l:%i %p') as 'Start Time',
            R.roomname,
            S.title,
            group_concat(P.pubsname SEPARATOR ', ') as 'Participants'
    FROM
            Schedule SCH
       JOIN Rooms R USING(roomid)
       JOIN Sessions S USING(sessionid)
       JOIN Tracks TR USING(trackid)
  LEFT JOIN ParticipantOnSession POS USING(sessionid)
  LEFT JOIN Participants P USING(badgeid)
    WHERE
            TR.trackname='FAST TRACK'
    GROUP BY
            SCH.scheduleid
    ORDER BY
            SCH.starttime"
