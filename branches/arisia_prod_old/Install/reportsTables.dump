	(43,'Conflict Report - Picky people ','Show who the picky people do not want to be on a panel with and who they are on panels with.','3a.',0,1,'conflictpickypeoplereport.php','<?xml version=\"1.0\" encoding=\"UTF-8\" ?>\n<xsl:stylesheet version=\"1.1\" xmlns:xsl=\"http://www.w3.org/1999/XSL/Transform\">\n    <xsl:output encoding=\"UTF-8\" indent=\"yes\" method=\"html\" />\n    <xsl:include href=\"xsl/reportInclude.xsl\" />\n    <xsl:template match=\"/\">\n        <xsl:choose>\n            <xsl:when test=\"doc/query[@queryName=\'participants\']/row\">\n                <table class=\"report\">\n                    <col style=\"width:6em;\" />\n                    <col style=\"width:12em;\" />\n                    <col />\n                    <col style=\"width:7em;\" />\n                    <col style=\"width:6em;\" />\n                    <col style=\"width:15em;\" />\n                    <col />\n                    <tr>\n                        <th class=\"report\">Badge ID</th>\n                        <th class=\"report\">Name for publications</th>\n                        <th class=\"report\">People to avoid</th>\n                        <th class=\"report\">Track</th>\n                        <th class=\"report\">Session ID</th>\n                        <th class=\"report\">Title</th>\n                        <th class=\"report\">Other participants</th>\n                    </tr>\n                    <xsl:apply-templates select=\"doc/query[@queryName=\'participants\']/row\"/>\n                </table>\n            </xsl:when>\n            <xsl:otherwise>\n                <div class=\"alert alert-danger\">No results found.</div>\n            </xsl:otherwise>                    \n        </xsl:choose>\n    </xsl:template>\n\n    <xsl:template match=\"doc/query[@queryName=\'participants\']/row\">\n        <tr>\n            <td class=\"report\">\n                <xsl:call-template name=\"showBadgeid\">\n                    <xsl:with-param name=\"badgeid\" select =