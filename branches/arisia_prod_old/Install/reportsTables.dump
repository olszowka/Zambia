VALUES
	(6,'Schedule','Below is the Panel, Events, Film, Anime, Video and Arisia TV schedule.','3a.',0,1,'2finalschedbreifreport.php','<?xml version=\"1.0\" encoding=\"UTF-8\" ?>\n<xsl:stylesheet version=\"1.1\" xmlns:xsl=\"http://www.w3.org/1999/XSL/Transform\">\n    <xsl:output encoding=\"UTF-8\" indent=\"yes\" method=\"html\" />\n    <xsl:include href=\"xsl/reportInclude.xsl\" />\n    <xsl:template match=\"/\">\n        <xsl:choose>\n            <xsl:when test=\"doc/query[@queryName=\'schedule\']/row\">\n                <table class=\"report\">\n                    <tr>\n                        <th class=\"report\">Start Time</th>\n                        <th class=\"report\">Duration</th>\n                        <th class=\"report\">Room Name</th>\n                        <th class=\"report\">Track Name</th>\n                        <th class=\"report\">Session ID</th>\n                        <th class=\"report\">Title</th>\n                    </tr>\n                    <xsl:apply-templates select=\"/doc/query[@queryName=\'schedule\']/row\"/>\n                </table>\n            </xsl:when>\n            <xsl:otherwise>\n                <div class=\"alert alert-danger\">No results found.</div>\n            </xsl:otherwise>                    \n        </xsl:choose>\n    </xsl:template>\n    <xsl:template match=\"/doc/query[@queryName=\'schedule\']/row\">\n        <tr>\n            <td class=\"report\"><xsl:value-of select=\"@starttime\" /></td>\n            <td class=\"report\">\n                <xsl:call-template name=\"showDuration\">\n                    <xsl:with-param name=\"durationhrs\" select = \"@durationhrs\" />\n                    <xsl:with-param name=\"durationmin\" select = \"@durationmin\" />\n                </xsl:call-template>\n            </td>\n            <td class=\"report\">\n                <xsl:call-template name=\"showRoomName\">\n                    <xsl:with-param name=\"roomid\" select = \"@roomid\" />\n                    <xsl:with-param name=\"roomname\" select = \"@roomname\" />\n                </xsl:call-template>\n            </td>\n            <td class=\"report\"><xsl:value-of select=\"@trackname\" /></td>\n            <td class=\"report\"><xsl:call-template name=\"showSessionid\"><xsl:with-param name=\"sessionid\" select = \"@sessionid\" /></xsl:call-template></td>\n            <td class=\"report\">\n                <xsl:call-template name=\"showSessionTitle\">\n                    <xsl:with-param name=\"sessionid\" select = \"@sessionid\" />\n                    <xsl:with-param name=\"title\" select = \"@title\" />\n                </xsl:call-template>\n            </td>\n        </tr>\n    </xsl:template>\n</xsl:stylesheet>\n',NULL,NULL,NULL),
	(7,'Schedule - brief diff','Recent changes to the PUBLIC Panel, Events, Film, Anime, Video and Arisia TV schedule.','3a.',0,1,'2finalschedbriefdiffreport.php','<?xml version=\"1.0\" encoding=\"UTF-8\" ?>\n<xsl:stylesheet version=\"1.1\" xmlns:xsl=\"http://www.w3.org/1999/XSL/Transform\">\n    <xsl:output encoding=\"UTF-8\" indent=\"yes\" method=\"html\" />\n    <xsl:include href=\"xsl/reportInclude.xsl\" />\n    <xsl:template match=\"/\">\n        <xsl:choose>\n            <xsl:when test=\"doc/query[@queryName=\'schedule\']/row\">\n                <table class=\"report\">\n                    <tr>\n                        <th class=\"report\">Start Time</th>\n                        <th class=\"report\">Duration</th>\n                        <th class=\"report\">Room Name</th>\n                        <th class=\"report\">Track Name</th>\n                        <th class=\"report\">Session ID</th>\n                        <th class=\"report\">Title</th>\n                        <th class=\"report\">When Changed</th>\n                        <th class=\"report\">Change Type</th>\n                    </tr>\n                    <xsl:apply-templates select=\"/doc/query[@queryName=\'schedule\']/row\"/>\n                </table>\n            </xsl:when>\n            <xsl:otherwise>\n                <div class=\"alert alert-danger\">No results found.</div>\n            </xsl:otherwise>                    \n        </xsl:choose>\n    </xsl:template>\n    <xsl:template match=\"/doc/query[@queryName=\'schedule\']/row\">\n        <tr>\n            <td class=\"report\"><xsl:value-of select=\"@starttime\" /></td>\n            <td class=\"report\">\n                <xsl:call-template name=\"showDuration\">\n                    <xsl:with-param name=\"durationhrs\" select = \"@durationhrs\" />\n                    <xsl:with-param name=\"durationmin\" select = \"@durationmin\" />\n                </xsl:call-template>\n            </td>\n            <td class=\"report\">\n                <xsl:call-template name=\"showRoomName\">\n                    <xsl:with-param name=\"roomid\" select = \"@roomid\" />\n                    <xsl:with-param name=\"roomname\" select = \"@roomname\" />\n                </xsl:call-template>\n            </td>\n            <td class=\"report\"><xsl:value-of select=\"@trackname\" /></td>\n            <td class=\"report\"><xsl:call-template name=\"showSessionid\"><xsl:with-param name=\"sessionid\" select = \"@sessionid\" /></xsl:call-template></td>\n            <td class=\"report\">\n                <xsl:call-template name=\"showSessionTitle\">\n                    <xsl:with-param name=\"sessionid\" select = \"@sessionid\" />\n                    <xsl:with-param name=\"title\" select = \"@title\" />\n                </xsl:call-template>\n            </td>\n            <td class=\"report\"><xsl:value-of select=\"@timestamp\" /></td>\n            <td class=\"report\"><xsl:value-of select=\"@description\" /></td>\n        </tr>\n    </xsl:template>\n</xsl:stylesheet>\n',NULL,NULL,NULL),
	(8,'Preliminary Programming Schedule ','Preliminary list of all scheduled \"Programming\" sessions.','3a.',0,1,'2prelimschedbriefreport.php','<?xml version=\"1.0\" encoding=\"UTF-8\" ?>\n<xsl:stylesheet version=\"1.1\" xmlns:xsl=\"http://www.w3.org/1999/XSL/Transform\">\n    <xsl:output encoding=\"UTF-8\" indent=\"yes\" method=\"html\" />\n    <xsl:include href=\"xsl/reportInclude.xsl\" />\n    <xsl:template match=\"/\">\n        <xsl:choose>\n            <xsl:when test=\"doc/query[@queryName=\'schedule\']/row\">\n                <table class=\"report\">\n                    <tr>\n                        <th class=\"report\">Start Time</th>\n                        <th class=\"report\">Duration</th>\n                        <th class=\"report\">Room Name</th>\n                        <th class=\"report\">Track Name</th>\n                        <th class=\"report\">Session ID</th>\n                        <th class=\"report\">Title</th>\n                        <th class=\"report\">When Changed</th>\n                        <th class=\"report\">Change Type</th>\n                    </tr>\n                    <xsl:apply-templates select=\"/doc/query[@queryName=\'schedule\']/row\"/>\n                </table>\n            </xsl:when>\n            <xsl:otherwise>\n                <div class=\"alert alert-danger\">No results found.</div>\n            </xsl:otherwise>                    \n        </xsl:choose>\n    </xsl:template>\n    <xsl:template match=\"/doc/query[@queryName=\'schedule\']/row\">\n        <tr>\n            <td class=\"report\"><xsl:value-of select=\"@starttime\" /></td>\n            <td class=\"report\">\n                <xsl:call-template name=\"showDuration\">\n                    <xsl:with-param name=\"durationhrs\" select = \"@durationhrs\" />\n                    <xsl:with-param name=\"durationmin\" select = \"@durationmin\" />\n                </xsl:call-template>\n            </td>\n            <td class=\"report\">\n                <xsl:call-template name=\"showRoomName\">\n                    <xsl:with-param name=\"roomid\" select = \"@roomid\" />\n                    <xsl:with-param name=\"roomname\" select = \"@roomname\" />\n                </xsl:call-template>\n            </td>\n            <td class=\"report\"><xsl:value-of select=\"@trackname\" /></td>\n            <td class=\"report\"><xsl:call-template name=\"showSessionid\"><xsl:with-param name=\"sessionid\" select = \"@sessionid\" /></xsl:call-template></td>\n            <td class=\"report\">\n                <xsl:call-template name=\"showSessionTitle\">\n                    <xsl:with-param name=\"sessionid\" select = \"@sessionid\" />\n                    <xsl:with-param name=\"title\" select = \"@title\" />\n                </xsl:call-template>\n            </td>\n            <td class=\"report\"><xsl:value-of select=\"@timestamp\" /></td>\n            <td class=\"report\"><xsl:value-of select=\"@description\" /></td>\n        </tr>\n    </xsl:template>\n</xsl:stylesheet>\n',NULL,NULL,NULL),
	(9,'CSV - Full Participant Schedule for the Program Packet Merge ','pubsname, (day, time, duration, room, mod)','2',1,1,'4progpacketmergereport.php',NULL,NULL,NULL,NULL),
	(10,'Program Participant Thank you note query','Name, mailing address, and number of scheduled sessions for programming sessions only.','3a.',0,1,'4progthankyounotereport.php','<?xml version=\"1.0\" encoding=\"UTF-8\" ?>\n<xsl:stylesheet version=\"1.1\" xmlns:xsl=\"http://www.w3.org/1999/XSL/Transform\">\n    <xsl:output encoding=\"UTF-8\" indent=\"yes\" method=\"html\" />\n    <xsl:include href=\"xsl/reportInclude.xsl\" />\n    <xsl:template match=\"/\">\n        <xsl:choose>\n            <xsl:when test=\"doc/query[@queryName=\'participants\']/row\">\n                <table class=\"report\">\n                    <tr>\n                        <th rowspan=\"2\" class=\"report\">Congo Name</th>\n                        <th rowspan=\"2\" class=\"report\">Pubs Name</th>\n                        <th rowspan=\"2\" class=\"report\">Badge Id</th>\n                        <th rowspan=\"2\" class=\"report\">Number of Sessions</th>\n                        <th colspan=\"6\" class=\"report\">Postal Address</th>\n                    </tr>\n                    <tr>\n                        <th class=\"report\">Address 1</th>\n                        <th class=\"report\">Address 2</th>\n                        <th class=\"report\">City</th>\n                        <th class=\"report\">State</th>\n                        <th class=\"report\">Zip</th>\n                        <th class=\"report\">Country</th>\n                    </tr>\n                    <xsl:apply-templates select=\"/doc/query[@queryName=\'participants\']/row\"/>\n                </table>\n            </xsl:when>\n            <xsl:otherwise>\n                <div class=\"alert alert-danger\">No results found.</div>\n            </xsl:otherwise>                    \n        </xsl:choose>\n    </xsl:template>\n\n    <xsl:template match=\"/doc/query[@queryName=\'participants\']/row\">\n        <tr>\n            <td class=\"report\"><xsl:value-of select=\"@firstname\"/><xsl:text disable-output-escaping=\"yes\">&amp;nbsp;</xsl:text><xsl:value-of select=\"@lastname\"/></td>\n            <td class=\"report\"><xsl:value-of select=\"@pubsname\"/></td>\n            <td class=\"report\"><xsl:call-template name=\"showBadgeid\"><xsl:with-param name=\"badgeid\" select=\"@badgeid\"/></xsl:call-template></td>\n            <td class=\"report\"><xsl:value-of select=\"@sessioncount\"/></td>\n            <td class=\"report\"><xsl:value-of select=\"@postaddress1\"/></td>\n            <td class=\"report\"><xsl:value-of select=\"@postaddress2\"/></td>\n            <td class=\"report\"><xsl:value-of select=\"@postcity\"/></td>\n            <td class=\"report\"><xsl:value-of select=\"@poststate\"/></td>\n            <td class=\"report\"><xsl:value-of select=\"@postzip\"/></td>\n            <td class=\"report\"><xsl:value-of select=\"@postcountry\"/></td>\n        </tr>\n    </xsl:template>\n</xsl:stylesheet>\n',NULL,NULL,NULL),
	(12,'Abridged Schedule Report','Lists all Sessions Scheduled in all Rooms.','3a.',0,1,'4pubsschedreport.php','<?xml version=\"1.0\" encoding=\"UTF-8\" ?>\r\n<xsl:stylesheet version=\"1.1\" xmlns:xsl=\"http://www.w3.org/1999/XSL/Transform\">\r\n    <xsl:output encoding=\"UTF-8\" indent=\"yes\" method=\"html\" />\r\n    <xsl:include href=\"xsl/reportInclude.xsl\" />\r\n    <xsl:template match=\"/\">\r\n        <xsl:choose>\r\n            <xsl:when test=\"doc/query[@queryName=\'sessions\']/row\">\r\n                <table class=\"report\">\r\n                    <tr>\r\n                        <th class=\"report\">Session ID</th>\r\n                        <th class=\"report\">Title</th>\r\n                        <th class=\"report\">Track</th>\r\n                        <th class=\"report\">Room</th>\r\n                        <th class=\"report\">StartTime</th>\r\n                        <th class=\"report\">Duration</th>\r\n                        <th class=\"report\">\r\n                            <div>Publication</div>\r\n                            <div>Status</div>\r\n                        </th>\r\n                        <th class=\"report\">Suitability for Children</th>\r\n                    </tr>\r\n                    <xsl:apply-templates select=\"doc/query[@queryName=\'sessions\']/row\"/>\r\n                </table>\r\n            </xsl:when>\r\n            <xsl:otherwise>\r\n                <div class=\"alert alert-danger\">No results found.</div>\r\n            </xsl:otherwise>                    \r\n        </xsl:choose>\r\n    </xsl:template>\r\n    <xsl:template match=\"doc/query[@queryName=\'sessions\']/row\">\r\n        <tr>\r\n            <td class=\"report\">\n				<xsl:call-template name=\"showSessionid\">\n					<xsl:with-param name=\"sessionid\" select=\"@sessionid\" />\n				</xsl:call-template>\n			</td>\r\n            <td class=\"report\">\n				<xsl:call-template name=\"showSessionTitle\">\n					<xsl:with-param name=\"sessionid\" select=\"@sessionid\" />\n					<xsl:with-param name=\"title\" select=\"@title\" />\n				</xsl:call-template>\n			</td>\r\n            <td class=\"report\"><xsl:value-of select=\"@trackname\" /></td>\r\n            <td class=\"report\">\n				<xsl:call-template name=\"showRoomName\">\n					<xsl:with-param name=\"roomid\" select=\"@roomid\" />\n					<xsl:with-param name=\"roomname\" select=\"@roomname\" />\n				</xsl:call-template>\n			</td>\r\n            <td class=\"report\"><xsl:value-of select=\"@starttime\" /></td>\r\n            <td class=\"report\">\r\n                <xsl:call-template name=\"showDuration\">\r\n                    <xsl:with-param name=\"durationhrs\" select = \"@durationhrs\" />\r\n                    <xsl:with-param name=\"durationmin\" select = \"@durationmin\" />\r\n                </xsl:call-template>\r\n            </td>\r\n            <td class=\"report\"><xsl:value-of select=\"@pubstatusname\" /></td>\r\n            <td class=\"report\"><xsl:value-of select=\"@kidscatname\" /></td>\r\n        </tr>\r\n    </xsl:template>\r\n</xsl:stylesheet>\r\n',NULL,NULL,NULL),
	(13,'Pubs - Who is on Which Session','Show the badgeid, pubsname and session info for each participant that are on at least one scheduled session. (Limited to published sessions.)','3a.',0,1,'4pubswhoisonwhichreport.php','<?xml version=\"1.0\" encoding=\"UTF-8\" ?>\n<xsl:stylesheet version=\"1.1\" xmlns:xsl=\"http://www.w3.org/1999/XSL/Transform\">\n    <xsl:output encoding=\"UTF-8\" indent=\"yes\" method=\"html\" />\n    <xsl:include href=\"xsl/reportInclude.xsl\" />\n    <xsl:template match=\"/\">\n        <xsl:choose>\n            <xsl:when test=\"doc/query[@queryName=\'participants\']/row\">\n                <table class=\"report\">\n                    <tr>\n                        <th class=\"report\" style=\"\">Badge ID</th>\n                        <th class=\"report\" style=\"\">Name for Publications</th>\n                        <th class=\"report\" style=\"\">List of Sessions</th>\n                    </tr>\n                    <xsl:apply-templates select=\"doc/query[@queryName=\'participants\']/row\" />\n                </table>\n            </xsl:when>\n            <xsl:otherwise>\n                <div class=\"alert alert-danger\">No results found.</div>\n            </xsl:otherwise>                    \n        </xsl:choose>\n    </xsl:template>\n\n    <xsl:template match=\"doc/query[@queryName=\'participants\']/row\">\n        <xsl:variable name=\"badgeid\" select=\"@badgeid\" />\n        <tr>\n            <td class=\"report\"><xsl:value-of select=\"@badgeid\" /></td>\n            <td class=\"report\">\n                <xsl:call-template name=\"showPubsname\">\n                    <xsl:with-param name=\"badgeid\" select = \"@badgeid\" />\n                    <xsl:with-param name=\"pubsname\" select = \"@pubsname\" />\n                </xsl:call-template>\n            </td>\n            <td class=\"report\">\n                <xsl:apply-templates select=\"/doc/query[@queryName=\'sessions\']/row[@badgeid=$badgeid]\" />\n            </td>\n        </tr>\n    </xsl:template>\n\n    <xsl:template match=\"doc/query[@queryName=\'sessions\']/row\">\n        <xsl:if test=\"position() > 1\">\n            <xsl:text>, </xsl:text>\n        </xsl:if>\n        <xsl:value-of select=\"@sessionid\" />\n        <xsl:if test=\"@moderator=\'1\'\">\n            <xsl:text> MOD</xsl:text>\n        </xsl:if>        \n    </xsl:template>\n</xsl:stylesheet>\n',NULL,NULL,NULL),
