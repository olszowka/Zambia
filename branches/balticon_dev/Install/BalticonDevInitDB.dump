-- INSERT INTO `EmailTo` VALUES (1,'Mike, Peter, Emily & Ben for testing',10,'SELECT CD.badgeid, CD.firstname, CD.lastname, CD.email, P.pubsname, CD.badgename\r\nfrom CongoDump CD join Participants P using (badgeid)\r\nwhere P.badgeid in (\'53159\',\'4440\',\'93133\',\'4720\') order by badgeid;'),(2,'All Program Participants',1,'SELECT\r\n            CD.badgeid, CD.firstname, CD.lastname, CD.email, P.pubsname, CD.badgename\r\n    from\r\n            CongoDump CD join\r\n            Participants P using (badgeid) join\r\n            UserHasPermissionRole UHPR using (badgeid)\r\n    where\r\n            UHPR.permroleid=3'),(3,'Program Participants who are attending',2,'SELECT CD.badgeid, CD.firstname, CD.lastname, CD.email, P.pubsname, CD.badgename\r\nfrom CongoDump CD, Participants P , UserHasPermissionRole UHPR\r\nwhere CD.badgeid=P.badgeid and CD.badgeid=UHPR.badgeid and UHPR.permroleid=3\r\nand P.interested=1'),(4,'Attending participants willing to read own work',3,'SELECT\r\n        CD.badgeid,\r\n        CD.firstname,\r\n        CD.lastname,\r\n        CD.email,\r\n        P.pubsname,\r\n        CD.badgename\r\n    from\r\n        CongoDump CD\r\n            join\r\n        Participants P\r\n            using (badgeid)\r\n            join\r\n        UserHasPermissionRole UHPR\r\n            using (badgeid)\r\n            join\r\n        ParticipantHasRole\r\n            using (badgeid)\r\n    where\r\n        UHPR.permroleid=3 and\r\n        P.interested=1 and\r\n        roleid = 2\r\n    order by lastname;'),(5,'Participants who participated in programming',9,'SELECT CD.badgeid, CD.firstname, CD.lastname, CD.email, P.pubsname, CD.badgename,\r\n    P.interested\r\n  from CongoDump CD inner join Participants P on CD.badgeid=P.badgeid\r\n    and CD.email is not null and P.badgeid in\r\n (select distinct badgeid from ParticipantOnSession POS inner join Sessions S\r\n    on POS.sessionid = S.sessionid and S.statusid=3 and S.divisionid=2);'),(6,'Attending participants willing to do workshop',4,'SELECT DISTINCT\r\n        CD.badgeid,\r\n        CD.firstname,\r\n        CD.lastname,\r\n        CD.email,\r\n        P.pubsname,\r\n        CD.badgename\r\n    from\r\n        CongoDump CD\r\n            join\r\n        (select badgeid from ParticipantHasRole where roleid = 6\r\n             union\r\n             select badgeid from ParticipantInterests where yespanels!=\"\") WP\r\n            using (badgeid)\r\n            join\r\n        Participants P\r\n            using (badgeid)\r\n            join\r\n        UserHasPermissionRole UHPR\r\n            using (badgeid)\r\n    where\r\n        P.interested=1 and\r\n        UHPR.permroleid=3\r\n    order by lastname;'),(7,'Have not logged in or responded to \"interested\" query',7,'SELECT CD.badgeid, CD.firstname, CD.lastname, CD.email, P.pubsname, CD.badgename FROM CongoDump CD JOIN Participants P USING (badgeid) JOIN UserHasPermissionRole UHPR USING (badgeid) WHERE UHPR.permroleid=3 AND P.interested IS NULL ORDER BY IFNULL(CD.lastname,\'ZZZZZ\');'),(8,'Staff with insecure passwords',8,'SELECT CD.badgeid, CD.firstname, CD.lastname, CD.email, P.pubsname, CD.badgename\r\nFROM CongoDump CD JOIN Participants P USING (badgeid) JOIN UserHasPermissionRole UHPR USING (badgeid)\r\nwhere UHPR.permroleid = 2 AND P.password =\'4cb9c8a8048fd02294477fcb1a41191a\';'),(9,'Scheduled Program Participants who are moderators',5,'SELECT DISTINCT\r\n        CD.badgeid,\r\n        CD.firstname,\r\n        CD.lastname,\r\n        CD.email,\r\n        P.pubsname,\r\n        CD.badgename\r\n    FROM\r\n            CongoDump CD\r\n       JOIN Participants P USING (badgeid)\r\n       JOIN UserHasPermissionRole UHPR USING (badgeid)\r\n       JOIN ParticipantOnSession POS ON (CD.badgeid = POS.badgeid AND POS.moderator = 1)\r\n       JOIN `Schedule` SCH USING (sessionid)\r\n       JOIN Sessions S ON (SCH.sessionid = S.sessionid AND S.divisionid = 2)\r\n     WHERE\r\n        UHPR.permroleid = 3\r\n    ORDER BY CD.lastname;');
