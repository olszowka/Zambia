TITLE='Session Interest Report (all info)'
DESCRIPTION='Shows who has expressed interest in each session, how they ranked it, what they said, if they will moderate... Large Report.  (All data included including for invited sessions.)'
PROGWANTS=1
QUERY="
SELECT
        T.trackname as 'Track',
        CONCAT('<A HREF=StaffAssignParticipants.php?selsess=',S.sessionid,'>',
                S.sessionid,'</A>') as Sessionid,
        CONCAT('<A HREF=EditSession.php?id=',S.sessionid,'>',title,'</A>') as title,
        P.pubsname,
        P.badgeid as 'BadgeID',
        PSI.rank as 'Rank',
        PSI.willmoderate as 'Mod?',
        PSI.comments as 'Comments'
    FROM
        Participants P,
        ParticipantSessionInterest PSI,
        Sessions S,
        Tracks T
    WHERE
        P.badgeid=PSI.badgeid AND
        S.sessionid=PSI.sessionid AND
        T.trackid=S.trackid
    ORDER BY T.trackname, title;"
