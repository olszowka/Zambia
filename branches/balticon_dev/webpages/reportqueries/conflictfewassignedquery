TITLE='Conflict Report - Sessions with under 4 people assigned'
DESCRIPTION='Not all of these are actually conflict, you want to think about them'
CONFLICTWANTS=1
QUERY="select trackname, concat('<a href=StaffAssignParticipants.php?selsess=',S.sessionid,'>', S.sessionid,'</a>') as Sessionid, title, count(badgeid) assigned from  Sessions S left join ParticipantOnSession POS on S.sessionid=POS.sessionid, Tracks T where T.trackid=S.trackid and S.statusid=3  group by sessionid having assigned<4 order by trackname, S.sessionid;"
