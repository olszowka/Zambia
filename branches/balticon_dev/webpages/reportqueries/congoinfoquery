TITLE='Congo Info (all info)'
DESCRIPTION='Shows the information retreived from Congo'
PROGWANTS=1
EVENTSWANTS=1
REGWANTS=1
ADMINWANTS=1
QUERY='select badgename, badgeid, regtype, lastname, firstname, phone, email, postaddress1, postaddress2, postcity, poststate, postzip, postcountry from CongoDump where badgeid is not NULL order by badgename;'
