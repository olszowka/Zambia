TITLE='FastTrack Schedule (easy troubleshooting)'
DESCRIPTION='What is happening in FastTrack'
FASTTRACKWANTS=1
QUERY=" Select DATE_FORMAT(ADDTIME('2009-01-16 00:00:00',SCH.starttime),'%a %l:%i %p') as 'Start Time', duration, R.roomname, S.title, group_concat(P.pubsname,' (',P.badgeid,')') as 'Participants' from Rooms R, Sessions S, Schedule SCH LEFT JOIN ParticipantOnSession POS on SCH.sessionid=POS.sessionid LEFT JOIN Participants P on POS.badgeid=P.badgeid where SCH.roomid = R.roomid and SCH.sessionid = S.sessionid and S.trackid=8 group by SCH.sessionid order by SCH.starttime;"
