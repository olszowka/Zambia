TITLE='Session Features'
DESCRIPTION='Which Session needs which Features?'
QUERY="select roomname,DATE_FORMAT(ADDTIME('2006-01-13 00:00:00',starttime),'%a %l:%i %p') as 'Start Time', duration,trackname, X.sessionid, title, X.featurename from (select duration,trackname,S.sessionid,title,featurename from Sessions S, SessionHasFeature SF, Features F, Tracks T where T.trackid=S.trackid and S.sessionid=SF.sessionid and F.featureid=SF.featureid) X, Rooms R, Schedule SCH where X.sessionid=SCH.sessionid and SCH.roomid=R.roomid order by roomname, starttime;"
