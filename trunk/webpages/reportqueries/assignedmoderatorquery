TITLE='Assigned Moderator by Session'
DESCRIPTION='Shows who has been assigned to moderate each session.'
PROGWANTS=1
EVENTSWANTS=1
QUERY='
select P.badgeid, 
       P.pubsname, 
       trackname, 
       Sessions.sessionid as SessionId, 
       title, 
       statusname 
  from ParticipantOnSession, 
       Sessions, 
       Participants P,
       Tracks, 
       SessionStatuses 
 where ParticipantOnSession.badgeid=P.badgeid 
   and ParticipantOnSession.sessionid=Sessions.sessionid 
   and Tracks.trackid=Sessions.trackid 
   and Sessions.statusid=SessionStatuses.statusid 
   and moderator=1 
order by trackname, 
         Sessions.sessionid;'
