TITLE='Session Services by Service'
DESCRIPTION='Which Session needs which Services?'
PROGWANTS=1
EVENTSWANTS=1
TECHWANTS=1
HOTELWANTS=1
QUERY="select roomname,DATE_FORMAT(ADDTIME('2008-01-18 00:00:00',starttime),'%a %l:%i %p') as 'Start Time', duration, trackname, X.sessionid, title, X.servicename from (select duration,trackname, S.sessionid,title,servicename from Tracks T, Sessions S, SessionHasService SF, Services F where T.trackid=S.trackid and S.sessionid=SF.sessionid and F.serviceid=SF.serviceid) X, Rooms R, Schedule SCH where X.sessionid=SCH.sessionid and SCH.roomid=R.roomid order by X.servicename, starttime;"
