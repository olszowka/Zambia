TITLE='Participant Kid Count'
DESCRIPTION='How many kids did the participants say they are bringing for Fast Track?'
FASTTRACKWANTS=1
QUERY='
    select P.Badgeid, 
           Pubsname, 
	   Numkidsfasttrack  
      from Participants P, 
           ParticipantAvailability PA 
     where P.badgeid=PA.badgeid
  order by Numkidsfasttrack desc;'


