TITLE='Full Room Schedule by time'
DESCRIPTION='Lists all Sessions Scheduled in all Rooms.'
QUERY="select roomname, function, DATE_FORMAT(ADDTIME('2006-01-13 00:00:00',starttime),'%a %l:%i %p') as 'Start Time', duration, title, group_concat(' ',firstname,' ',lastname,' (',C.badgeid,')') as 'Participants' from Rooms R, Sessions S, Schedule SCH LEFT JOIN ParticipantOnSession POS ON SCH.sessionid=POS.sessionid LEFT JOIN CongoDump C ON POS.badgeid=C.badgeid where SCH.roomid = R.roomid and SCH.sessionid = S.sessionid group by SCH.sessionid order by SCH.starttime, R.roomname"
