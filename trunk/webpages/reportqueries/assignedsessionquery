TITLE='Assigned Session by Session'
DESCRIPTION='Shows who has been assigned to each session.'
PROGWANTS=1
EVENTSWANTS=1
QUERY="
select P.pubsname,
       if ((moderator=1), 'Yes', ' ') as 'moderator',
       trackname, 
       Sessions.sessionid as SessionId, 
       title, 
       statusname 
  from ParticipantOnSession, 
       Sessions, 
       Participants P, 
       Tracks, 
       SessionStatuses 
where ParticipantOnSession.badgeid=P.badgeid 
  and ParticipantOnSession.sessionid=Sessions.sessionid 
  and Tracks.trackid=Sessions.trackid 
  and Sessions.statusid=SessionStatuses.statusid 
order by trackname, 
         Sessions.sessionid;"
