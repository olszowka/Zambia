TITLE='Assigned Session by Participant'
DESCRIPTION='Shows who has been assigned to each session ordered by badgeid.'
PROGWANTS=1
EVENTSWANTS=1
QUERY='select CongoDump.badgeid, badgename, Sessions.sessionid as SessionId, title, moderator from ParticipantOnSession, Sessions, CongoDump where ParticipantOnSession.badgeid=CongoDump.badgeid and ParticipantOnSession.sessionid=Sessions.sessionid order by cast(CongoDump.badgeid as unsigned); '
