TITLE='Full Room Schedule'
DESCRIPTION='Lists all Sessions Scheduled in all Rooms.'
QUERY="select roomname, function, DATE_FORMAT(ADDTIME('2006-01-13 00:00:00',starttime),'%a %l:%i %p') as 'Start Time', duration, title, group_concat(' ',firstname,' ',lastname,' (',C.badgeid,')') as 'Participants' from Rooms R, Sessions S, Schedule SCH,CongoDump C, ParticipantOnSession POS where SCH.roomid = R.roomid and SCH.sessionid = S.sessionid and C.badgeid=POS.badgeid and POS.sessionid=SCH.sessionid group by SCH.sessionid order by R.roomname, SCH.starttime;"
