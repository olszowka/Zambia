TITLE='Tech Grid'
DESCRIPTION='Not really sure what tech wants.  email us, please (zambia@arisia.org). '
QUERY="select DATE_FORMAT(ADDTIME('2006-01-13 00:00:00',starttime),'%a %l:%i %p') as 'Start Time', group_concat(if(roomid='2',Concat(trackname,' ',sessionid,' ',title,' (',left(duration,5),')',roomid,' '),'') separator '') as 'Presidents ABCD',group_concat(if(roomid='3',Concat(trackname,' ',sessionid,' ',title,' (',left(duration,5),')',roomid,' '),'') separator '') as 'Presidents A',group_concat(if(roomid='4',Concat(trackname,' ',sessionid,' ',title,' (',left(duration,5),')',roomid,' '),'') separator '') as 'Presidents B',group_concat(if(roomid='6',Concat(trackname,' ',sessionid,' ',title,' (',left(duration,5),')',roomid,' '),'') separator '') as 'Presidents D',group_concat(if(roomid='45',Concat(trackname,' ',sessionid,' ',title,' (',left(duration,5),')',roomid,' '),'') separator '') as 'Presidents AB' from (SELECT trackname,' ',S.sessionid, SCH.roomid, starttime, duration, title from Schedule SCH, Sessions S, Rooms R, Tracks T where T.trackid=S.trackid and T.trackname != 'Do Not Print' and S.sessionid = SCH.sessionid and SCH.roomid=R.roomid and R.function like '%ent%')as Z group by starttime;"
