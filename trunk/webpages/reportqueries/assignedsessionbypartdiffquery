TITLE='Differential Assigned Session by Participant'
DESCRIPTION='Recent changes to whom has been assigned to each session ordered by badgeid.'
PROGWANTS=1
EVENTSWANTS=1
GOHWANTS=1
PUBSWANTS=1
QUERY="
select P.badgeid, 
       P.pubsname, 
       Sessions.sessionid as SessionId, 
       title, 
       if ((moderator=1), 'Yes', ' ') as 'moderator'
  from ParticipantOnSession POS, 
       Sessions, 
       Participants P 
 where POS.badgeid=P.badgeid 
   and POS.sessionid=Sessions.sessionid 
   and POS.ts>'2008-01-18 23:00:00'
order by cast(P.badgeid as unsigned);"
